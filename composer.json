{
    "name": "antoniosiles/nova-4-card-map-plus",
    "description": "A Laravel Nova card plus.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0 || ^11.0"
    },
    "require-dev": {
        "laravel/nova": "^4.0 || ^5.0",
        "laravel/nova-devtool": "^1.1"
    },
    "repositories": [
        { "type": "composer", "url": "https://nova.laravel.com" }
    ],
    "autoload": {
        "psr-4": {
            "AntonioSiles\\Nova4CardMap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntonioSiles\\Nova4CardMap\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
